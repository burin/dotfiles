export CC=/usr/local/bin/gcc-4.2
export EDITOR=bbedit

# for fasd
eval "$(fasd --init auto)"

# for node
export NVM_DIR="/Users/sean/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"

# for ruby
export RBENV="$HOME/.rbenv/bin"
eval "$(rbenv init -)"

export PATH=~/bin:$RBENV:$PATH

